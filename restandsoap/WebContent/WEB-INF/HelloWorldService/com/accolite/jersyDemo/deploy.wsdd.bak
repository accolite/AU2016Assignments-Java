<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from HelloWorldService WSDL service -->

  <service name="HelloWorld" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://jersyDemo.accolite.com"/>
      <parameter name="wsdlServiceElement" value="HelloWorldService"/>
      <parameter name="schemaQualified" value="http://jersyDemo.accolite.com"/>
      <parameter name="wsdlServicePort" value="HelloWorld"/>
      <parameter name="className" value="com.accolite.jersyDemo.HelloWorldSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="HelloWorld"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="addition" qname="operNS:addition" xmlns:operNS="http://jersyDemo.accolite.com" returnQName="retNS:additionReturn" xmlns:retNS="http://jersyDemo.accolite.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://jersyDemo.accolite.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://jersyDemo.accolite.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="printHello" qname="operNS:printHello" xmlns:operNS="http://jersyDemo.accolite.com" returnQName="retNS:printHelloReturn" xmlns:retNS="http://jersyDemo.accolite.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="showEmployee" qname="operNS:ShowEmployee" xmlns:operNS="http://jersyDemo.accolite.com" returnQName="retNS:ShowEmployeeReturn" xmlns:retNS="http://jersyDemo.accolite.com" returnType="rtns:Employee" xmlns:rtns="http://jersyDemo.accolite.com" soapAction="" >
        <parameter qname="pns:employee" xmlns:pns="http://jersyDemo.accolite.com" type="tns:Employee" xmlns:tns="http://jersyDemo.accolite.com"/>
      </operation>
      <parameter name="allowedMethods" value="showEmployee printHello addition"/>

      <typeMapping
        xmlns:ns="http://jersyDemo.accolite.com"
        qname="ns:Employee"
        type="java:com.accolite.jersyDemo.Employee"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
